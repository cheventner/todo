{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./js", "README.md"],
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./template"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
